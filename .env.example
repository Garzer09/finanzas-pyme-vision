# Environment Variables Configuration
# Copy this file to .env and fill in the actual values

# Environment Type (development, production, test)
VITE_ENVIRONMENT=development

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Debug and Logging
VITE_DEBUG_MODE=false
VITE_ENABLE_LOGGING=true
VITE_LOG_LEVEL=INFO

# Application Metadata
VITE_APP_VERSION=1.0.0

# Performance and Caching
VITE_ENABLE_CACHE=true
VITE_CACHE_TTL=3600

# Security Settings
VITE_ENABLE_RATE_LIMITING=true
VITE_RATE_LIMIT_MAX_ATTEMPTS=5
VITE_RATE_LIMIT_WINDOW_MS=900000
VITE_RATE_LIMIT_BLOCK_DURATION_MS=3600000
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=.xlsx,.xls,.csv
VITE_ENABLE_CSRF_PROTECTION=true
VITE_SESSION_TIMEOUT_MS=1800000

# Analytics and Monitoring
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_HEALTH_MONITORING=true
VITE_HEALTH_CHECK_INTERVAL_MS=300000
VITE_SENTRY_DSN=your_sentry_dsn_optional

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_NEW_FEATURES=false
VITE_EXCEL_PARSER_MODE=development
VITE_ENABLE_SECURITY_LOGGING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true